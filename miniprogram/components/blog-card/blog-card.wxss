/* components/blog-card/blog-card.wxss */
.blog {
  padding: 20rpx;
  background-color: #fff;
}

.blog-title {
  display: flex;
}

.blog-portrait {
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.blog-box {
  flex: 1;
}

.blog-time {
  font-size: 24rpx;
  color: #939393;
}