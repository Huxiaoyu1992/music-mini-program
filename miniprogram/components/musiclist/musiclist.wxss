/* components/musiclist/musiclist.wxss */
.musiclist-container {
  display:flex;
  align-items: center;
  background: honeydew;
  width: 100%;
  padding: 10rpx 20rpx;
  font-size: 29rpx;
  color: rgb(85, 80, 80);
}

.musiclist-index {
  width: 8%;
  font-size: 34rpx;
}

.music-info {
  flex-grow: 1; /*规定项目相对于其他灵活的项目进行扩展的量。默认值是 0*/
  width: 0; /*当内容超出的时候做的设置*/
}

.musiclist-name {
  text-shadow: lawngreen;
  font-size: 34rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
}



